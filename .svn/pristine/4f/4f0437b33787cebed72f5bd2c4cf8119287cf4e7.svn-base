.warn {
    color: #b3b3b3;
    display: flex;
    justify-content: space-around;
    align-items: center;
    border-bottom: 1px solid #eee;
    border-top: 1px solid #eee;
    padding: 10px 0;
    margin-bottom: 15px;
    font-size: 30px;
    justify-content: center;
}

.warn img{
    height: 30px;
    width: 30px;
    margin: 0 10px;
}
